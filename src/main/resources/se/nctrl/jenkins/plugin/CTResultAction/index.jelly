<script src="${HUDSON_CONTEXT_PATH}/plugin/jenkins_ct/g.raphael/g.line-min.js" type="text/javascript" charset="utf-8"></script>
<script src="${HUDSON_CONTEXT_PATH}/plugin/jenkins_ct/g.raphael/g.line-min.js" type="text/javascript" charset="utf-8"></script>
<script src="${HUDSON_CONTEXT_PATH}/plugin/jenkins_ct/g.raphael/g.pie-min.js" type="text/javascript" charset="utf-8"></script>


<script type="text/javascript">
                var num_passed = ${it.passCount};
                var num_failed = ${it.failCount};
                var num_skipped = ${it.skipCount};
</script>

<script type="text/javascript">

<![CDATA[


$j(document).ready(function () {
                


function filterGlobal (str)
{
  
      
      for (var  i = 0;i < $j('.dataTable').size(); i++)
      {
        
        $j("#dt_"+i).dataTable().fnFilter(
          str,
          null,
          true,
          true
      );
    }
}


                var r = Raphael("result_chart",380,250);
                var passed_col = "#14D100";
                var failed_col = "#FFC200";
                var skipped_col = "#FF2300";

               
                pie = r.piechart(125, 125, 100, 
                        [num_passed,num_failed,num_skipped],
                        { 
                          colors: [passed_col,failed_col,skipped_col], 
                          strokewidth: "3",
                          legend: ["Passed", "Failed", "Skipped"],
                          legendpos: "east"
                        }
                        );
                


                $j('.table').dataTable( { 
                  "iDisplayLength" : 50,
                  "sDom" : "<'row-fluid'<'span6'T><'span6'<'control-group'<'controls'f>>>r>t<'row-fluid'<'span6'i><'span6 pull-right'p>>",
                  "aaSorting": [[2,'asc']]
                });
                $j('.dataTables_wrapper').removeClass("form-inline");
                $j('.dataTables_filter > label').contents().filter(function() { return this.nodeType == 3; }).remove(); // Remove label text
                $j('.dataTables_filter > label > input').unwrap();
                $j('.dataTables_filter').addClass("input-prepend");
                $j('.dataTables_filter > input').before("<span class='add-on'><i class='icon-filter'></i></span>");
                $j('.dataTables_filter > input').attr('id','inputIcon').attr('placeholder','filter').addClass("span8");

                // Global filter
                $j('#gfilter_all > a').bind('click',function(event){ event.preventDefault(); $j("[id^=gfilter]").removeClass("active");$j('#gfilter_all').addClass("active"); filterGlobal("")});
                $j('#gfilter_failed > a').bind('click',function(event){ event.preventDefault(); $j("[id^=gfilter]").removeClass("active");$j('#gfilter_failed').addClass("active");  filterGlobal("Failed")});
                $j('#gfilter_passed > a').bind('click',function(event){ event.preventDefault();$j("[id^=gfilter]").removeClass("active");$j('#gfilter_passed').addClass("active");  filterGlobal("Passed")});
                $j('#gfilter_skipped > a').bind('click',function(event){ event.preventDefault();$j("[id^=gfilter]").removeClass("active");$j('#gfilter_skipped').addClass("active"); filterGlobal("Skipped")});


                // Navbar
                $j("#nav-back").click(function() { history.back(); return false; });

                // Show sorting order in table headers
                $j("th[class^=sorting]").bind('click',function(event)
                  {
                      var up = "icon-arrow-up";
                      var down = "icon-arrow-down"
                      var i = $j(event.target).children('i');

                      
                      $j(i).toggleClass(up,$j(event.target).hasClass("sorting_asc")).toggleClass(down,$j(event.target).hasClass("sorting_desc"));
                      $j("th[class=sorting] > i").toggleClass(up,false).toggleClass(down,false);
                  }

                  );

                  $j("#ajaxb").bind('click',function(event) { 
                    foo.ajaxTest(function(r) { 
                                    alert(r.responseObject()); 
                                  }
                                ); 
                  });

                  $j('[id^=c_]').bind('click',function(event) {

                    
                    var e_id = $j(event.target).attr("id");
                    var box = $j("#box_" + e_id)
                    if (!box.hasClass("loaded")) { box.addClass("loaded") }
                    box.toggleClass("closed");

                    box.toggle("slow");

                    if (box.hasClass("closed"))
                    {
                      $j("#" + e_id).text("+")
                    } else { $j("#" + e_id).text("-") }


                  });
                  

 
]]> 
                });
</script>




</head>
 <body>

    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="#">CT Results</a>
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li><a href="#" id="nav-back"><i class="icon-chevron-left icon-white"></i> Go back</a></li>
              <!--
              <li><a href="#">Project</a></li>
              <li><a href="#">Export</a></li>
               -->              
            </ul>

          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <div class="container">

      <!-- Summary -->
      <div class="well small">
        
        <div class="row">
            <div class="span5">
            <div id="result_chart"></div>
          </div>
          <div class="span5">
          <p class="lead"><h3>Test results, summary</h3>
          <hr />
          <dl class="dl-horizontal">
          <dt>Number of suites</dt>
          <dd>${it.result.children.size()}</dd>
          <dt>Number of test cases</dt>
          <dd>${it.totalCount}</dd>
          <dt>Passed test cases</dt>
          <dd>${it.passCount}</dd>
          <dt>Failed test cases</dt>
          <dd>${it.failCount}</dd>
          <dt>Skipped test cases</dt>
          <dd>${it.skipCount}</dd>
          </dl>
          
           <button class="btn btn-large btn-primary" type="button" id="ajaxb">Test</button>
           
          </p>
          </div>

        </div>
      </div>
      <!-- Summary -->

      <!-- Content -->



<div class="navbar">
  <div class="navbar-inner">
    <a class="brand" href="#">Filter</a>
    <ul class="nav">
      <li class="active" id="gfilter_all"><a href="#">Show all</a></li>
      <li id="gfilter_passed"><a href="#" >Only Passed</a></li>
      <li id="gfilter_failed"><a href="#" >Only Failed</a></li>
      <li id="gfilter_skipped"><a href="#">Only Skipped</a></li>
    </ul>
  </div>
</div>





      <j:forEach var="table" items="${it.report_layout.tables}">

      <div class="row">
        <div class="span12">
          <p class="lead"><button class="btn btn-mini" type="button" id="c_${table.table_id}">-</button> ${table.label}
            <!-- <br /> --> &#160;&#160;
          <j:if test="${table.passCount != 0}">
          <span class="badge badge-success">${table.passCount}</span>
          </j:if>
          <j:if test="${table.failCount != 0}">
          &#160;<span class="badge badge-important">${table.failCount}</span>
          </j:if>
          <j:if test="${table.skipCount != 0}">
          &#160;<span class="badge badge-warning">${table.skipCount}</span></j:if>&#160;</p>
          
          <hr />
         <div class="table_box" id="box_c_${table.table_id}"> 
         <table class="table table-striped" id="dt_${table.table_id}">
  
  <thead>
    <tr>
      <th>Result <i class="icon-empty"></i></th>
      <th>Test case <i class="icon-empty"></i></th>
      <th>Started <i class="icon-empty icon-arrow-up"></i></th>
    </tr>
  </thead>
  <tbody>

<!-- cases -->
        <j:forEach var="row" items="${table.rows}">
    <tr>
      <td>
          <j:if test="${row.result == 0}">
          <span class="label label-important">Failed</span>
          </j:if>
          <j:if test="${row.result == 1}">
          <span class="label label-success">Passed</span>
          </j:if>
          <j:if test="${row.result == 2}">
          <span class="label label-warning">Skipped</span>
          </j:if>

      </td>
      <td>${row.displayName}</td>
      <td>${row.started.toString()}</td>
    </tr>
        </j:forEach>
<!-- cases -->

  </tbody>
</table> 
</div> <!-- table_box -->
        </div>
      
      

 </div> <!-- Content -->         
      </j:forEach> <!-- tables -->



      <hr />

      <footer>
        <p>Company 2012</p>
      </footer>

    </div> <!-- container -->

</body></html>

      
</j:jelly>